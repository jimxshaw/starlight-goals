    <!--
        Calculating opacity values example:
        29% opacity of 255 is about 74.
        74% opacity of 255 is about 189.
        Int 74 converted to hex is 4A.
        Int 189 converted to hex is BD.
        4A000000 would be 29% opacity for black.
        BDFFFFFF would be 74% opacity for white.
    -->
    <color name="recycler_view_row_divider">#EDFAA000</color>
    <color name="background_recycler_view_row_goal_light">#4AFAA000</color>
    <color name="background_recycler_view_row_goal_dark">#BDFAA000</color>
    <color name="background_recycler_view_row_goal_completed">#EDFAA000</color>


</resources>
